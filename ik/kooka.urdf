<robot name = "kooka">
    <link name = "lower-base" />
    <link name = "upper-base" />
    <link name = "above-elbow" />
    <link name = "below-elbow" />

    <joint name = "swivel" type="revolute">
        <parent link = "lower-base"/>
        <child link = "upper-base" />
        <limit lower = "-0.785398" upper = "0.785398" />
        <axis xyz = "NEED_TO_SPECIFY" />
    </joint>

    <joint name = "shoulder" type="revolute">
        <parent link = "upper-base"/>
        <child link = "above-elbow" />
        <limit lower = "0" upper ="2.35619" />
        <axis xyz = "NEED_TO_SPECIFY"/>
    </joint>

    <joint name = "elbow" type="revolute">
        <parent link = "above-elbow"/>
        <child link = "below-base" />
        <limit lower = "0" upper ="2.35619" />
        <axis xyz = "NEED_TO_SPECIFY"/>
    </joint>
</robot>